#  ^${capabilities_enabled}(,$$|$$) -> Continue.Record 
  
#  ^\s{6}Capabilities Enabled        = ${capabilities_enabled} -> CHECKEND
#  ^.${capabilities_enabled}$$ -> Record Start
#  ^\s{6}Capabilities Enabled        = ${capabilities_enabled},*$$ -> Start

# CHECKEND
#   ^\s*$$ -> Record Start
#   ^\s*,$$ -> Start
#   ^. -> Error

#  ^\s*\=\s*${capabilities_enabled}$$ -> LLDP
  # ^\s{6}Capabilities Enabled        = ${capabilities_enabled},*$$ -> Continue
#  ^\s{6}Capabilities Enabled        = ${capabilities_enabled}$$ -> Record

# MANAGEMENT  
#   ^\s{6}Management IP Address       = ${management_ip_address},$$ -> Continue.Record
#   ^\s{6}Management IP Address       = ${management_ip_address}$$ -> Record

# REMOTEPORTDEFAULTVLAN
#   ^\s{6}Remote port default vlan    = ${remote_port_default_vlan},$$ -> VLANID
#   ^\s{6}Remote port default vlan    = ${remote_port_default_vlan}$$ -> Record Start

# VLANID
#   ^\s{6}Vlan ID                     = ${vlan_id},$$ -> VLANNAME
#   ^\s{6}Vlan ID                     = ${vlan_id}$$ -> Record Start

# VLANNAME
